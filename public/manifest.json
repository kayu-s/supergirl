{
  "name": "supergirl",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Make it easy to use GitHub",
  "icons": {
    "16": "icons/supergirl@2x.png",
    "48": "icons/supergirl@2x.png",
    "128": "icons/supergirl@2x.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  }
}
